#include<stdio.h>
int main(){
int a, b , c;
printf("Quais os numeros?");
scanf("%d, %d, %d", &a, &b, &c);
if (a > b && b > c){printf("os numeros em orem crescente: %d %d %d", &a &b &c);}
if (a < b && b < c){printf("os numeros em ordem crescente: %d %d %d", &c &b %a);}
if (a > b && b < c){printf("os numeros em ordem crescente: %d %d %d", &a, &c &b);}
}
